{
  "name": "@erk/ackermann",
  "version": "1.0.0",
  "description": "https://en.wikipedia.org/wiki/Ackermann_function",
  "main": "src/index.js",
  "keywords": [
    "ackermann"
  ],
  "author": "Eric Heayn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericheayn/ackermann/issues"
  },
  "homepage": "https://github.com/ericheayn/ackermann#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericheayn/ackermann.git"
  },
  "scripts": {
    "test": "jest --coverage",
    "coverage": "cat ./test/coverage/lcov.info | coveralls",
    "lint": "eslint ./src"
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "<rootDir>/test/unit/*.test.js"
